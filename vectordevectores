#include <iostream>
#include <conio.h>
#define TAM 100

using namespace std;

struct elemento
{ int vec[TAM];
};

void cargar (elemento vect[], int n)
{int i, j;
for(i=0;i<n;i++)
  {for(j=0;j<n;j++)
{cout<<"INGRESE UN VALOR: ";
 cin>>vect[i].vec[j];
}
}
}


void mostrar(elemento vect[], int n)
{int i, j;
for(i=0;i<n;i++)
  {for(j=0;j<n;j++)
{cout<<vect[i].vec[j];
}
cout<<endl;
}
}


void main()
{elemento vect[TAM];
 int n;
 cout<<"Ingrese el tamaÃ±o del vector: ";
 cin>>n;
 cargar(vect,n);
 mostrar(vect,n);
 getch();
}
